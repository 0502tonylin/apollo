<!--this file list the modules which will be loaded dynamicly and their process name to be running in -->

<cybertron>
	<desc>cybertron modules list config</desc>
	<version>1.0.0</version>

	<module>
		<name>gnss</name>
		<dag_conf>/apollo/modules/drivers/gnss/dag/gnss.dag</dag_conf>
		<process_name>apollo</process_name>
		<version>1.0.0</version>
	</module>

	<module>
		<name>velodyne</name>
		<dag_conf>/apollo/modules/drivers/velodyne/dag/velodyne.dag</dag_conf>
		<process_name>apollo</process_name>
		<version>1.0.0</version>
	</module>

	<module>
        <name>localization</name>
        <dag_conf>/apollo/modules/localization/dag/localization.dag</dag_conf>
        <process_name>apollo</process_name>
        <version>1.0.0</version>
    </module>
	
	<module>
		<name>canbus</name>
		<dag_conf>/apollo/modules/canbus/dag/canbus.dag</dag_conf>
		<process_name>apollo-control</process_name>
		<version>1.0.0</version>
	</module>

	<module>
		<name>control</name>
		<dag_conf>/apollo/modules/control/dag/control.dag</dag_conf>
		<process_name>apollo-control</process_name>
		<version>1.0.0</version>
	</module>

	<module>
		<name>guardian</name>
		<dag_conf>/apollo/modules/guardian/dag/guardian.dag</dag_conf>
		<process_name>apollo-control</process_name>
		<version>1.0.0</version>
	</module>

</cybertron>
