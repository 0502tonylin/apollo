# Define all coms in DAG streaming.
module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/camera/libcamera_component.so"

    components {
      class_name : "UsbCamComponent"
      config {
        name : "camera_driver_short"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_short_conf.pb.txt"
      }
    }

    components {
      class_name : "UsbCamComponent"
      config {
        name : "camera_driver_long"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_long_conf.pb.txt"
      }
    }

    components {
      class_name : "UsbCamComponent"
      config {
        name : "camera_driver_front"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_front_conf.pb.txt"
      }
    }

}

